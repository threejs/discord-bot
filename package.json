  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.23.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "discord.js": "12.5.3",
    "dompurify": "^2.2.7",
    "dotenv": "8.2.0",
    "fuzzysort": "^1.1.4",
    "jsdom": "^16.5.2",
    "node-fetch": "^2.6.1",
    "puppeteer": "^8.0.0"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src",
    "serve": "babel-node src",
